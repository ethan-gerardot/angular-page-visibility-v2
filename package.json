  "scripts": {
    "ng": "ng",
    "prestart": "npm run build:lib",
    "start": "ng serve",
    "prebuild": "npm run build:lib",
    "build": "ng build",
    "build:lib": "ng build --configuration production angular-page-visibility-v2-lib && cp *.md dist/angular-page-visibility-v2",
    "test": "ng test",
    "lint": "ng lint",
    "publish": "npm publish dist/angular-page-visibility-v2",
    "e2e": "ng e2e",
    "release": "standard-version",
    "release:major": "npm run release -- -t '' --release-as major",
    "release:minor": "npm run release -- -t '' --release-as minor",
    "release:patch": "npm run release -- -t '' --release-as patch"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ethan-gerardot/angular-page-visibility-v2.git"
  },
  "author": "Olivier LIN-SI-CHENG",
  "bugs": {
    "url": "https://github.com/ethan-gerardot/angular-page-visibility-v2/issues"
  },
  "homepage": "https://github.com//angular-page-visibility-v2#readme",
  "dependencies": {
    "@angular/animations": "^20.3.7",
    "@angular/common": "^20.3.7",
    "@angular/compiler": "^20.3.7",
    "@angular/core": "^20.3.7",
    "@angular/forms": "^20.3.7",
    "@angular/platform-browser": "^20.3.7",
    "@angular/platform-browser-dynamic": "^20.3.7",
    "@angular/router": "^20.3.7",
    "ajv": "^6.9.1",
    "core-js": "2.6.5",
    "rxjs": "^7.5.7",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.7",
    "@angular/cli": "^20.3.7",
    "@angular/compiler-cli": "^20.3.7",
    "@angular/language-service": "^20.3.7",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~11.13.2",
    "husky": "^2.2.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^20.3.0",
    "prettier": "1.16.4",
    "pretty-quick": "^1.10.0",
    "protractor": "~7.0.0",
    "standard-version": "^8.0.1",
    "ts-node": "~8.0.3",
    "tslib": "^2.4.1",
    "tslint": "~5.15.0",
    "typescript": "~5.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
